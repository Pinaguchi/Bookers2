<%= render 'books/info', {book: @book, user: @user_new} %>

<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <% @users.each do |user| %>
  <tbody>
    <tr>
      <td><%= image_tag user.get_profile_image("40x40") %></td>
      <td><%= user.name %></td>
      <td>
        <%= link_to "Show", user_path(current_user) %>
      </td>
    </tr>
  </tbody>
  <% end %>
<table>
</div>

</div>
</div>